<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.isoler.studyim.business.chatroom.mapper.ChatRoomMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.isoler.studyim.business.chatroom.model.bean.ChatRoom">
        <id column="c_id" property="id" />
        <result column="c_name" property="name" />
        <result column="c_topic" property="topic" />
        <result column="c_description" property="description" />
        <result column="dt_create_time" property="createTime" />
        <result column="dt_update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        c_id, c_name, c_topic, c_description, dt_create_time, dt_update_time
    </sql>

</mapper>
